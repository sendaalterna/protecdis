        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "h3, h4, p, span": {
              "margin": "0"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            ".item-box-text, .item-content": {
              "padding-bottom": "1rem"
            },
            "p": {
              "font-weight": "300"
            },
            ".item:focus, span:focus": {
              "outline": "none"
            },
            ".item-text": {
              "padding-top": "1rem"
            },
            ".item": {
              "&:hover": {
                "cursor": "pointer"
              },
              ".item-box-text, .cur, .item-box-price": {
                "&:hover": {
                  "cursor": "default"
                }
              }
            },
            ".item-wrapper, .item-box, .item-img": {
              "position": "relative"
            },
            ".item-btn, .item-box-btn": {
              "margin": "0"
            },
            ".item-box-title": {
              "color": "#1576a6"
            },
            ".amp-shop__title": {
              "text-align": "center"
            },
            ".item-box-btn": {
              "display": "inline-block"
            },
            ".item-box": {
              "justify-content": "center",
              "width": "100%",
              "background": "#efefef",
              "overflow": "auto",
              "@media (min-width: 992px)": {
                "max-width": "1000px"
              },
              "@media (max-width: 991px)": {
                "width": "90%",
                "margin": "1rem",
                "padding-top": "3rem"
              }
            },
            ".item-box-img": {
              "align-items": "center",
              "amp-img": {
                "height": "auto"
              }
            },
            ".item-content": {
              "text-align": "center"
            },
            ".item-box-wrapper": {
              "background-color": "#efefef"
            },
            ".close:before, .close:after": {
              "background-color": "#777777"
            },
            "& when (@customStyles)": {
              "& when (@addPadding)": {
                "& when not (@imgPadding)": {
                  ".item-wrapper": {
                    "padding": "(@yPadding * 1rem) (@xPadding * 1rem)"
                  }
                },
                "& when (@imgPadding)": {
                  ".item-img": {
                    "padding-bottom": "0"
                  },
                  ".item-content": {
                    "padding": "(@yPadding * 1rem) (@xPadding * 1rem)"
                  }
                }
              },
              "& when (@boxShadow)": {
                ".item-wrapper": {
                  "box-shadow": "0px 0px (@shadowSize * 1px) 0px rgba(red(@shadowColor), green(@shadowColor), blue(@shadowColor), @shadowOpacity)"
                }
              },
              "& when (@useColorCard)": {
                ".item-wrapper": {
                  "position": "relative",
                  "z-index": "0",
                  "&:after": {
                    "content": "\"\"",
                    "display": "block",
                    "position": "absolute",
                    "top": "0",
                    "bottom": "0",
                    "left": "0",
                    "right": "0",
                    "opacity": "@opacityCard",
                    "background": "@colorCard",
                    "transition": "opacity .3s",
                    "pointer-events": "none",
                    "z-index": "-1"
                  }
                }
              },
              "& when (@useBorderRadius)": {
                ".item-wrapper": {
                  "z-index": "12",
                  "border-radius": "(@borderRadius * 1px)",
                  "overflow": "hidden"
                }
              },
              ".item-wrapper": {
                "color": "@fontColor"
              },
              ".btn": {
                "background-color": "@btnColor !important",
                "border-color": "@btnColor !important",
                "color": "contrast(@btnColor, black, white, 30%) !important"
              },
              ".btn:hover": {
                "background-color": "@btnHoverColor !important",
                "border-color": "@btnHoverColor !important",
                "color": "contrast(@btnHoverColor, black, white, 30%) !important"
              }
            },
            ".item-box-price": {
              "color": "#fb5f3d"
            },
            ".cur": {
              "color": "#fb5f3d"
            }
          },
          "_name": "shop01",
          "_customHTML": "<section class=\"amp-shop\" group=\"Shop\" plugins=\"amp-main, amp-lightbox, amp-bind\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"8\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Title\" name=\"titleSection\" checked>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" name=\"showText\" title=\"Show Text\">\n\n        <header condition=\"customStyle\">Custom Styles</header>\n        <input type=\"checkbox\" name=\"customStyles\" title=\"Custom Style\" checked>\n\n        <header condition=\"customStyles\">Card Color</header>\n        <input type=\"checkbox\" name=\"useColorCard\" title=\"Use Background Card\" condition=\"customStyles\" checked>\n        <input type=\"color\" name=\"colorCard\" title=\"Color\" value=\"#bbd2d7\" condition=\"useColorCard && customStyles\">\n        <input type=\"range\" title=\"Opacity\" name=\"opacityCard\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\" condition=\"useColorCard && customStyles\">\n        <input type=\"color\" name=\"fontColor\" title=\"Font Color\" value=\"#000000\" condition=\"customStyles\">\n        <input type=\"color\" name=\"btnColor\" title=\"Btn Color\" value=\"#0dcd7b\" condition=\"customStyles\">\n        <input type=\"color\" name=\"btnHoverColor\" title=\"Hover Btn Color\" value=\"#04854e\" condition=\"customStyles\">\n\n        <header condition=\"customStyles\">Paddings</header>\n        <input type=\"checkbox\" name=\"addPadding\" title=\"Add Padding\" condition=\"customStyles\" checked>\n        <input type=\"checkbox\" name=\"imgPadding\" title=\"No Image Padding\" condition=\"customStyles && addPadding\" checked>\n        <input type=\"range\" title=\"X Padding\" name=\"xPadding\" min=\"0\" max=\"5\" step=\"0.5\" value=\"1\" condition=\"addPadding && customStyles\">\n        <input type=\"range\" title=\"Y Padding\" name=\"yPadding\" min=\"0\" max=\"5\" step=\"0.5\" value=\"1.5\" condition=\"addPadding && customStyles\">\n\n        <header condition=\"customStyles\">Box Shadow</header>\n        <input type=\"checkbox\" name=\"boxShadow\" title=\"Shadow\" condition=\"customStyles\" checked>\n        <input type=\"range\" name=\"shadowSize\" title=\"Size\" min=\"0\" max=\"100\" step=\"1\" value=\"15\" condition=\"customStyles && boxShadow\">\n        <input type=\"range\" name=\"shadowOpacity\" title=\"Opacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"customStyles && boxShadow\">\n        <input type=\"color\" name=\"shadowColor\" title=\"Color\" condition=\"customStyles && boxShadow\" value=\"#212529\">\n\n        <header condition=\"customStyles\">Border Radius</header>\n        <input type=\"checkbox\" name=\"useBorderRadius\" title=\"Round Border\" condition=\"customStyles\" checked>\n        <input type=\"range\" name=\"borderRadius\" title=\"Size\" min=\"0\" max=\"50\" step=\"1\" value=\"10\" condition=\"customStyles && useBorderRadius\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" required>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#fff0ed\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"amp-shop\">\n            <h2 mbr-text mbr-theme-style=\"display-2\" class=\"amp-shop__title mbr-pb-4 mbr-semibold mbr-fonts-style\" mbr-if=\"titleSection\"><b>Tenemos Mascarillas FFP2 NR D - KN95</b></h2>\n            <div class=\"mbr-row\">\n                <!--1-->\n         \n                <div on=\"tap:showLightbox4\" role=\"button\" tabindex=\"0\" class=\"item shop-image mbr-col-lg-4 mbr-col-md-12 mbr-col-sm-12 mbr-pb-5\">\n                    <amp-lightbox id=\"showLightbox1\" layout=\"nodisplay\">\n                        <div class=\"lightbox\">\n                            <div class=\"item-box mbr-row\">\n                                <button type=\"button\" class=\"close\" aria-label=\"Close\" on=\"tap:showLightbox3.close\">\n                                    <span aria-hidden=\"true\"></span>\n                                </button>\n                                <div class=\"item-box-img mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-flex mbr-p-4\">\n                                    <amp-img src=\"@PROJECT_PATH@/assets/images/mascarillas-600x400.png\" layout=\"responsive\" width=\"150\" height=\"100\" alt=\"image\" class=\"placeholder-loader\">\n                                        <div placeholder class=\"placeholder\"></div>\n                                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/mascarillas-600x400.png\" alt title>\n                                    </amp-img>\n                                </div>\n                                <div class=\"item-box-wrapper mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-p-4\">\n\n                                    <div class=\"item-box-header\">\n                                        <h4 class=\"item-box-title mbr-fonts-style mbr-pb-1\" mbr-theme-style=\"display-5\" data-app-selector=\".item-box-title\"><b>Protectores KN95</b></h4>\n                                        <div class=\"currency mbr-pb-1\">\n                                            <span mbr-text class=\"cur mbr-fonts-style\" data-app-selector=\".cur\" mbr-theme-style=\"display-5\"><b>S/.</b></span>\n                                            <span mbr-text class=\"item-box-price mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".item-box-price\"><b>11.90</b></span>\n                                        </div>\n\n                                    </div>\n                                    <div class=\"item-box-body\">\n                                        <p class=\"item-box-text mbr-regular mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-box-text\">\n                                            Estas mascarillas de protección FFP2 pretenden ofrecer una barrera física que en la mayoría de los casos no requiere válvulas de exhalación. El objetivo principal es evitar la propagación de aire, saliva o, en algunos casos, el contagio por sangre.</p>\n                                    </div>\n                                    <div class=\"item-box-footer\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd\" mbr-buttons mbr-theme-style=\"display-4\"><a href=\"https://api.whatsapp.com/send?phone=51969064844&text=Hola%20por%20favor%20cont%C3%A1ctame%20me%20interesan%20tus%20productos\" class=\"btn item-box-btn btn-success\" target=\"_blank\" data-app-placeholder=\"Type Text\">WhatsApp</a></div>\n                                </div>\n                            </div>\n                        </div>\n                    </amp-lightbox>\n                    <!--Item-->\n                    <div class=\"item-wrapper\">\n                        <div class=\"item-img\">\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/mascarillas-600x400.png\" layout=\"responsive\" width=\"337.5\" height=\"225\" alt=\"image\" class=\"placeholder-loader\">\n                                <div placeholder class=\"placeholder\"></div>\n                                <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/mascarillas-600x400.png\" alt>\n                            </amp-img>\n                        </div>\n                        <div class=\"item-content mbr-pt-3\">\n                            <h4 class=\"item-box-title item-title mbr-fonts-style mbr-pb-1\" data-app-selector=\".item-box-title\" mbr-theme-style=\"display-7\"><b>Protectores KN95</b></h4>\n                            <span class=\"cur mbr-fonts-style\" mbr-theme-style=\"display-7\"><b>S/.</b></span>\n                            <span class=\"item-price mbr-fonts-style\" mbr-theme-style=\"display-7\"><b>11.90</b></span>\n                            <p class=\"item-text mbr-regular\" mbr-if=\"showText\" mbr-theme-style=\"display-7\">\n                                            Estas mascarillas de protección FFP2 pretenden ofrecer una barrera física que en la mayoría de los casos no requiere válvulas de exhalación. El objetivo principal es evitar la propagación de aire, saliva o, en algunos casos, el contagio por sangre.</p>\n                        </div>\n                    </div>\n                </div><div on=\"tap:showLightbox4\" role=\"button\" tabindex=\"0\" class=\"item shop-image mbr-col-lg-4 mbr-col-md-12 mbr-col-sm-12 mbr-pb-5\">\n                    <amp-lightbox id=\"showLightbox1\" layout=\"nodisplay\">\n                        <div class=\"lightbox\">\n                            <div class=\"item-box mbr-row\">\n                                <button type=\"button\" class=\"close\" aria-label=\"Close\" on=\"tap:showLightbox3.close\">\n                                    <span aria-hidden=\"true\"></span>\n                                </button>\n                                <div class=\"item-box-img mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-flex mbr-p-4\">\n                                    <amp-img src=\"@PROJECT_PATH@/assets/images/mascarillas-2-600x400.png\" layout=\"responsive\" width=\"150\" height=\"100\" alt=\"image\" class=\"placeholder-loader\">\n                                        <div placeholder class=\"placeholder\"></div>\n                                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/mascarillas-2-600x400.png\" alt title>\n                                    </amp-img>\n                                </div>\n                                <div class=\"item-box-wrapper mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-p-4\">\n\n                                    <div class=\"item-box-header\">\n                                        <h4 class=\"item-box-title mbr-fonts-style mbr-pb-1\" mbr-theme-style=\"display-5\" data-app-selector=\".item-box-title\">\n<div><b>Mascarillas FFP2 NR D</b><br></div></h4>\n                                        <div class=\"currency mbr-pb-1\">\n                                            <span mbr-text class=\"cur mbr-fonts-style\" data-app-selector=\".cur\" mbr-theme-style=\"display-5\"><b>S/.</b></span>\n                                            <span mbr-text class=\"item-box-price mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".item-box-price\"><b>11.90</b></span>\n                                        </div>\n\n                                    </div>\n                                    <div class=\"item-box-body\">\n                                        <p class=\"item-box-text mbr-regular mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-box-text\">\n                                            Las Mascarillas FFP2 de protección son muy utilizadas para la salvaguardia respiratoria de las personas contra la contaminación del aire, también para tratamientos médicos e igualmente para evitar que las partículas de aire y las bacterias entren en nuestro sistema.</p>\n                                    </div>\n                                    <div class=\"it